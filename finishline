// LEFT TRACK CHANNEL START LINE

//linetrackchannelleft//
integer linetrackchannelleftnumber = 13;
key linetrackchannelleftkey = "f60670d0-16ce-43cf-8ec6-edd4ecc361d9";
integer linetrackchannelleft() {
    string seti = "phrasego";
    key     channelkey    = linetrackchannelleftkey;
    integer channelnumber = linetrackchannelleftnumber;
    string setint = (string)((integer)("0x"+llGetSubString((string)channelkey,-8,-1)) * llStringLength(seti) * llCeil(llLog(llStringLength(seti)))*channelnumber);
    return (integer)llGetSubString(setint, 0, 7);
}
//          0         //


default
{
    
    state_entry(){  llVolumeDetect(TRUE); }
  collision_end(integer num)
    {
       key  detectedracer = llDetectedOwner(0);  
        
        
         llRegionSay(linetrackchannelleft(),"finish|"+(string)detectedracer);}
}
